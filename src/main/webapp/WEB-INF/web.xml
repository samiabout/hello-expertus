<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>HelloExpertus</servlet-name>
        <servlet-class>servlet.SignForm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Disconnect</servlet-name>
        <servlet-class>servlet.Disconnect</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TSP</servlet-name>
        <servlet-class>servlet.TspForm</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>HelloExpertus</servlet-name>
        <url-pattern>/expertus</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>Disconnect</servlet-name>
        <url-pattern>/disconnect</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TSP</servlet-name>
        <url-pattern>/expertusTSP</url-pattern>
    </servlet-mapping>

</web-app>


